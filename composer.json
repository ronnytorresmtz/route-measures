{
    "name": "ronnytorresmtz/route-measures",
    "description": "A Laravel Nova field - Calculate the distance and duration of a route",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ronnytorresmtz\\RouteMeasures\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ronnytorresmtz\\RouteMeasures\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
